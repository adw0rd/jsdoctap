{
  "name": "jsdoctap",
  "description": "Doctests using JSDOC examples and Node-Tap",
  "version": "0.0.1",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "WTFPL",
  "main": "lib/index.js",
  "bin": "bin/index.js",
  "scripts": {
    "test": "bash run-examples"
  },
  "homepage": "https://github.com/zacanger/jsdoctap#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/jsdoctap.git"
  },
  "bugs": "https://github.com/zacanger/jsdoctap/issues",
  "keywords": [
    "doctests",
    "jsdoc",
    "node-tap",
    "tap",
    "test",
    "testing",
    "workflow"
  ],
  "dependencies": {
    "commander": "^2.8.1",
    "dox": "^0.8",
    "js-beautify": "^1.5.4",
    "lodash": "^4.0.1",
    "should": "^10.0.0",
    "tap": "10.7.1",
    "uglify-js": "^3.0.13"
  },
  "devDependencies": {
    "babel-register": "^6.9.0",
    "blanket": "1.1.7",
    "bluebird": "^3.2.1",
    "mocha": "^3.3.0",
    "mocha-spec-cov-alt": "^1.1.1"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules"
      ],
      "pattern": [
        "lib",
        "bin"
      ],
      "spec-cov": {
        "threshold": 70,
        "lcovOutput": "lcov"
      }
    }
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
