{
  "name": "jsdoctap",
  "description": "Test runner for doctests using JSDoc examples and node-tap.",
  "version": "0.3.8",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "WTFPL",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": "bin/index.js",
  "scripts": {
    "test": "npm run test:lint && npm run test:tap && npm run test:examples",
    "test:examples": "./bin/index.js examples/*.js | tap-spec",
    "test:lib": "./bin/index.js lib/*.js | tap-spec",
    "test:lint": "eslint -c .eslintrc.json lib bin",
    "test:tap": "tap lib/*.test.js | tap-spec"
  },
  "homepage": "https://github.com/zacanger/jsdoctap#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/jsdoctap.git"
  },
  "bugs": "https://github.com/zacanger/jsdoctap/issues",
  "keywords": [
    "doctest",
    "doctests",
    "documentation",
    "jsdoc",
    "node-tap",
    "tap",
    "test",
    "testing",
    "workflow"
  ],
  "dependencies": {
    "babel-core": "6.26.0",
    "dox": "0.9.0",
    "require-from-string": "1.2.1",
    "tap": "10.7.2",
    "zeelib": "3.0.8"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "4.6.1",
    "eslint-config-jane": "0.5.3",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-unicorn": "2.1.2",
    "tap-spec": "4.1.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
